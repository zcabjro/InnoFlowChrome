{
	"manifest_version": 2,
	"name": "InnoFlow",
	"version": "0.1",

	"browser_action": {
		"default_popup": "popup.html"
	},

	"content_security_policy": "script-src 'self'; object-src 'self'",

	"permissions": [
    "https://innoflow.herokuapp.com/"
  ],

	"icons": { 
		"16": "icons/16x16.png",
		"32": "icons/32x32.png",
    "48": "icons/48x48.png",
    "128": "icons/128x128.png"
	}
}